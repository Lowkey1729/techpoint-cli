{
    "name": "zeevx/techpoint-cli",
    "description": "Read all the latest Techpoint headlines right from your terminal.",
    "keywords": ["framework", "laravel", "techpoint", "console", "cli"],
    "homepage": "https://techpoint.ng",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/zeevx/techpoint-cli/issues",
        "source": "https://github.com/zeevx/techpoint-cli"
    },
    "authors": [{
        "name": "Adams Paul",
        "email": "adamsohiani@gmail.com"
    }],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.5.0",
        "illuminate/http": "^8.0",
        "laminas/laminas-text": "^2.7",
        "laravel-zero/framework": "^8.0",
        "nunomaduro/laravel-console-menu": "^3.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["techpoint"]
}
